@model IEnumerable<db_first_exam.Models.assignprostuproVM>
@{
    Layout = null;
}

@*<!DOCTYPE html*@

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>Information About Project</h2>
    <div> 
        <table border="1">
            <tr>
                <td colspan="3">
                    @using (Html.BeginForm("searchbyproject", "Test", FormMethod.Post))
                    {
                        @:Category:
                        @Html.DropDownList("projectid", "Select project")
                        <input type="submit" value="Search" />
                    }
                </td>
            </tr>
            <tr>
                
                <td>projectname</td>
                <td>studentcount</td>
            </tr>
           @foreach(var temp in Model)
            {
                <tr>
                    
                    <td>@temp.projectname</td>
                    <td>@temp.studentid</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
