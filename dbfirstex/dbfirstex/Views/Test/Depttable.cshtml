@model IEnumerable<dbfirstex.Models.customertbl>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Depttable</title>
</head>
<body>
    <div>
        <table border="=1">
            <tr>
                <td>Customer Id</td>
                <td>Customer Name</td>
                <td>Customer Address</td>
                <td>Customer Email Id</td>
                <td>Customer Mobile No</td>
            </tr>
            @foreach (var temp in Model)
            {
                <tr>
                    <td>@temp.customerid</td>
                    <td>@temp.customername</td>
                    <td>@temp.address</td>
                    <td>@temp.emailid</td>
                    <td>@temp.mobileno</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
