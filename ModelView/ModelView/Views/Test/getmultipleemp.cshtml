@model IEnumerable<ModelView.Models.emp>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>getmultipleemp</title>
</head>
<body>
    <div>
        <h2>employee information</h2>
        <table border="1">
            <tr>
       <td> Employee Id</td>
       <td> Employee Name</td>
       <td> Employee Department</td>
       <td> Employee Mobile No</td>
       <td> Employee Salary</td>
                </tr>
            @foreach(var temp in Model)
            {
                <tr>
                    <td>@temp.empid</td>
                    <td>@temp.empname</td>
                    <td>@temp.empdept</td>
                    <td>@temp.empmobile</td>
                    <td>@temp.empsalary</td>
                 </tr>
            }
            </table>
    </div>
</body>
</html>
